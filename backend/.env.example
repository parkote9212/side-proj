# JWT Configuration
# Generate a strong secret key for production (minimum 256 bits / 32 characters)
JWT_SECRET_KEY=your-jwt-secret-key-here
JWT_EXPIRATION_TIME=3600000

# Server Configuration
SERVER_PORT=8080

# Onbid API Configuration (Camco)
# Get your service key from https://www.data.go.kr/
ONBID_API_BASEURL=http://openapi.onbid.co.kr/openapi/services/KamcoPblsalThingInquireSvc
ONBID_API_SERVICEKEY=your-onbid-service-key-here

# Kakao API Configuration
# Get your REST API key from https://developers.kakao.com/
KAKAO_API_BASEURL=https://dapi.kakao.com
KAKAO_API_RESTAPIKEY=your-kakao-rest-api-key-here

# Database Configuration (Production)
# For development, H2 in-memory database is used by default
# Uncomment and configure for production environment with MariaDB
# SPRING_DATASOURCE_URL=jdbc:mariadb://localhost:3306/onbid_db
# SPRING_DATASOURCE_USERNAME=root
# SPRING_DATASOURCE_PASSWORD=your_password_here
# SPRING_DATASOURCE_DRIVER_CLASS_NAME=org.mariadb.jdbc.Driver

# Batch Configuration (Optional)
# BATCH_MANUAL_TRIGGER_ENABLED=false

